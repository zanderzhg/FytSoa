                        <!-- 快捷方式 -->
                        <div class="soa-user-down">
                            <div class="user-set-item"><a href=""><i class="fa fa-cog"></i>基本资料</a></div>
                            <div class="user-set-item"><a href=""><i class="fa fa-edit"></i>修改密码</a></div>
                            <div class="user-set-item"><a href=""><i class="fa fa-eraser"></i>清除缓存</a></div>
                            <div class="user-set-item"><a href="/fytadmin/index/logout/"><i class="fa fa-sign-in"></i>退出</a></div>
                        </div>
                    </li>
	                    	
                    <li><a href="javascript:void(0)" class="clearCache"><i class="fa fa-eraser"></i>　清除缓存</a></li>
                    <li class="sel-site">
                        <a href="javascript:void(0)"><i class="fa fa-cubes"></i>　飞易腾官网2017<b></b></a>
