                    <div class="soa-tabs-scroller-left"></div>
                    <div class="soa-tabs-scroller-right"></div>
                    <div class="soa-tabs-wrap">
                        <a href="javascript:void(0)" class="mem-close"><i class="fa fa-bars"></i></a>
                    </div>
                </div>
            </div>
            <!-- end -->
            <div class="soa-h50 soa-top-rmenu">
                <ul>
                    <li class="log-user">
