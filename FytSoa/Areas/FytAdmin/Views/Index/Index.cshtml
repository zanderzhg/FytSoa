</div>
<div id="ifload"><div style="margin-top:300px;"><img src="/Content/wwwroot/lib/img/status.gif" style="vertical-align:middle" />&nbsp;&nbsp;正在加载，请稍后...</div></div>
@Scripts.Render("~/fytadmin/basejs")
@Scripts.Render("~/fytadmin/menujs")
