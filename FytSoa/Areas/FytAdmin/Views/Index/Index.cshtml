    <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no,minimal-ui">
    <!-- 360浏览器默认使用Webkit内核 -->
    <meta name="renderer" content="webkit">
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <title>飞易腾后台管理系统</title>
    @Styles.Render("~/fytadmin/basecss")
    @Styles.Render("~/fytadmin/menucss")
    @Styles.Render("~/fytadmin/styles")
</head>
<body style="overflow: hidden;">
<!-- 头部开始 -->
<div class="soa-in-header clearfix">
    <div class="soa-in-tleft">
        <div class="soa-w225 soa-h50 soa-title">飞易腾后台管理系统</div>
    </div>
    <div class="soa-in-main">
        <div class="soa-h50 soa-top-nav">
